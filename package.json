{
    "name": "code-mood",
    "displayName": "Code Mood",
    "description": "Enhance your coding experience with ambient scenes and mood-based themes",
    "version": "0.0.1",
    "publisher": "vscode-samples",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-extension-samples"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "code-mood.toggleMood",
                "title": "Code Mood: Toggle Mood"
            },
            {
                "command": "code-mood.showQuote",
                "title": "Code Mood: Show Inspirational Quote"
            },
            {
                "command": "code-mood.toggleAmbient",
                "title": "Code Mood: Toggle Ambient Scene"
            }
        ],
        "keybindings": [
            {
                "command": "code-mood.toggleMood",
                "key": "ctrl+alt+m",
                "mac": "cmd+alt+m"
            },
            {
                "command": "code-mood.showQuote",
                "key": "ctrl+alt+q",
                "mac": "cmd+alt+q"
            },
            {
                "command": "code-mood.toggleAmbient",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "typescript": "^5.3.3",
        "@vscode/test-electron": "^2.3.8"
    },
    "dependencies": {}
}